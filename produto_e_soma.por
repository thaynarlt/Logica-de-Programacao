programa {

      //Escreva um algoritmo que obtenha dois números inteiros, mostre o produto e a soma entre eles.
  funcao inicio() {
    inteiro num1, num2, produto, soma

    escreva("Digite dois números inteiros:")
    leia(num1, num2)

    produto = num1 * num2
    soma = num1 + num2

    escreva("O produto dos números digitado é: ", produto)
    escreva("\nA soma dos números digitados é: ", soma)

  }
}
