programa
{
	
 		//Fazer um algoritmo que converta centímetros para polegadas
			// obs: (1 pol = 2.54cm)
	funcao inicio()
	{
	
		real cent, polegadas
	
		escreva("Digite um valor qualquer em centímetros: ")
		leia(cent)

		polegadas = cent / 2.54

		escreva("O valor correspondido em polegadas é: ", polegadas)
		
	}
}
